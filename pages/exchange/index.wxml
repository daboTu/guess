<wxs module="dateFr" src="../../filter/dateFr.wxs"></wxs>
<block wx:for="{{list}}" wx:key="{{index}}" wx:for-item='item'>
  <view class='exchabgeBox'>
    <view class='img'>
      <image src='{{item.url}}'></image>
    </view>
    <view class='info'>
      <view class='title'>{{item.giftName}}</view>
      <view class='integral'>积分：{{item.integral}}</view>
      <view class='integral'>兑换时间：{{dateFr.getDate(item.createTime)}}</view>
      <view class='integral'>订单号：23456789234567</view>
    </view>
  </view>
</block>