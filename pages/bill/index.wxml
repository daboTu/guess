<wxs module="dateFr" src="../../filter/dateFr.wxs"></wxs>

<block wx:for="{{list}}" wx:key="{{index}}" wx:for-item='item'>
  <view class='list' catchtap='toexchange'>
    <view class='icon'>
      <text class='fa fa-yelp' aria-hidden="true"></text>
    </view>
    <view class='text'>
      <view>商品兑换</view>
      <view class='time'>{{dateFr.getDate(item.createTime)}}</view>
    </view>
    <view class='right'>
      {{item.integral}}
    </view>
  </view>
</block>